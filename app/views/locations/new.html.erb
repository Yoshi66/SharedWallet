    <div class="field">
      <%= u.label :address %><br>
      <%= u.text_field :address %>
    </div>
    <div class="field">
      <%= u.label :photo %><br>
      <%= u.file_field :photo %>
    </div>
  <div class="actions">
    <%= u.submit 'create pin'%>
    <%#<%= f.submit 'Update Tag', :name => 'update' %>
  </div>
<% end %>