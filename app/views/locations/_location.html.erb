<li>
  <%= link_to location.address, '#' %>
  <% if location.photo.present?%>
    <p><%= image_tag(location.photo.url(:medium)) %></p>
  <% end %>
  <% if @project.locations.last.id == location.id %>
    <% if Project.find(@project.id).locations.last.user_id == current_user.id.to_s %>
      <% if @project.locations.first != location %>
      <div align="right">
        <%= link_to 'Edit Pin',  edit_project_location_path(@project.id, location) %>
        <%= link_to "delete", [@project, location], :method  => :delete %>
      </div>
      <% else %>
      <div align="right">
        <%= link_to 'Edit', edit_project_path(@project) %> |
        <%= link_to 'Destroy', project_path, :method => :delete %> |
      </div>
      <% end %>
    <% end %>
  <% end %>
  <%#<%= debug(@project.locations.last.user_id)%>
  <%#<%= debug(Project.find(@project.id).locations.last.user_id) %>
  <%#<%= debug(current_user.id.to_s) %>
</li>